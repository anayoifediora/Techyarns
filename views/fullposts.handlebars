<div class="card">
  <div class="card-header">
    {{dataValues.createdAt}}
  </div>
  <div class="card-body">
    <h5 class="card-title">{{dataValues.title}}</h5>
    <p class="card-text">Author:{{dataValues.user}} </p>
    <p class="card-text">{{dataValues.description}} </p>
    <a href="/posts/{{id}}" class="btn btn-primary">Comments</a>
  </div>
</div>